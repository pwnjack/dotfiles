{
   "author": "Qwant",
   "background": {
      "page": "pages/background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": {
         "19": "assets/icons/green-19.png",
         "38": "assets/icons/green-38.png"
      },
      "default_popup": "pages/popup.html",
      "default_title": "__MSG_name__"
   },
   "chrome_settings_overrides": {
      "search_provider": {
         "encoding": "UTF-8",
         "favicon_url": "https://www.qwant.com/favicon.ico",
         "is_default": true,
         "keyword": "www.qwant.com",
         "name": "Qwant",
         "search_url": "https://www.qwant.com/?q={searchTerms}&client=ext-chrome-sb",
         "suggest_url": "https://api.qwant.com/api/suggest/?q={searchTerms}&client=opensearch"
      }
   },
   "content_scripts": [ {
      "css": [ "assets/css/no-popin.css" ],
      "matches": [ "https://*.qwant.com/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src 'self' 'sha256-CA6h8X2PlfpQX4tMUn1T0JjKVVC7t6WCcAxnAPhndGk='; object-src 'self'",
   "default_locale": "en",
   "description": "__MSG_description__",
   "icons": {
      "128": "assets/icons/green-128.png",
      "16": "assets/icons/green-16.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "79.0",
   "name": "__MSG_name__",
   "omnibox": {
      "keyword": "q"
   },
   "optional_permissions": [ "\u003Call_urls>", "webNavigation" ],
   "options_page": "pages/options.html",
   "permissions": [ "webRequest", "webRequestBlocking", "unlimitedStorage", "storage" ],
   "short_name": "__MSG_short_name__",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "7.0.5",
   "web_accessible_resources": [ "/web-accessible-resources/*" ]
}
